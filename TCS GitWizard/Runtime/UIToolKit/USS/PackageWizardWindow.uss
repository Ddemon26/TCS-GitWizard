* {
    -unity-text-outline-width: 0.3px;
    -unity-text-outline-color: rgb(0, 0, 0);
    letter-spacing: 2px;
    text-shadow: 1px 0.5px 0 rgb(0, 0, 0);
    font-size: 14px;
    color: rgb(255, 255, 255);
    -unity-font-style: bold;
    text-overflow: ellipsis;
}

.main-header {
    -unity-font-definition: url("project://database/Assets/_Project/Settings/TextMesh%20Pro/Examples%20&%20Extras/Fonts/Unity.ttf?fileID=12800000&guid=f4eec857a4fdf2f43be0e9f3d1a984e7&type=3#Unity");
    -unity-text-align: middle-center;
    white-space: nowrap;
    font-size: 30px;
    -unity-font-style: bold;
    text-overflow: ellipsis;
}

.group-border {
