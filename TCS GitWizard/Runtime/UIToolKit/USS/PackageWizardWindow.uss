}

.unity-button:hover {
    transition-duration: 0.2s;
    transition-timing-function: ease-in-out;
    scale: 1.1 1.1;
    border-left-color: rgba(105, 212, 61, 0.31);
    border-right-color: rgba(105, 212, 61, 0.31);
    border-top-color: rgba(105, 212, 61, 0.31);
    border-bottom-color: rgba(105, 212, 61, 0.31);
}

.unity-button:active {
    transition-duration: 0.1s;
    transition-timing-function: linear;
    background-color: rgb(46, 200, 64);
    border-left-color: rgb(34, 156, 204);
    border-right-color: rgb(34, 156, 204);
    border-top-color: rgb(34, 156, 204);
    border-bottom-color: rgb(34, 156, 204);
    border-top-width: 2px;
    border-right-width: 2px;
    border-bottom-width: 2px;
    border-left-width: 2px;
    font-size: 7px;
}
